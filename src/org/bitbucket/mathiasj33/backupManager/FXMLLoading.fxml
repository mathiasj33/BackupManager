<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<AnchorPane id="AnchorPane" prefHeight="147.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="140.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="322.0" text="Backing up your files">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="17.0" />
               </font></Label>
            <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></ProgressBar>
            <Label fx:id="progressLabel">
               <VBox.margin>
                  <Insets left="180.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="closeButton" disable="true" mnemonicParsing="false" onAction="#close" text="Close">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
   <stylesheets>
       <URL value="@JMetroLightTheme.css" />
   </stylesheets>
</AnchorPane>
